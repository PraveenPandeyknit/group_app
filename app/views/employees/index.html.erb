<div>
<h2>Employees :</h2>
<table>
	<thead>
	  <tr>
	    <th>Id</th>
	    <th>Name</th>
	    <th>Department Name</th>
	    <th>Email</th>
	    <th>Adress</th>
	  </tr>
	</thead>
	<% @employees.each do |employee|%>
	  <tr>
	    <td><%=employee.id%></td>
	    <td><%=employee.name%></td>
	    <td><%=employee.department_name%></td>
	    <td><%=employee.email%></td>
	    <td><%=employee.address%></td>
	  </tr>
	<%end%>
</table>
</div>